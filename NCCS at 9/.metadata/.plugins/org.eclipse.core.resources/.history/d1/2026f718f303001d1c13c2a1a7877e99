package coreJava.basics.OOP.concepts.inheritance;

public class App {
	public static void main(String[] args) {
		Employee emp = new Employee();
		emp.nationalId = 4134214;
		emp.name = "Rahul Gotame";
		emp.address = "ktm";
		emp.empId=43;
		emp.dept = "accounts";
		emp.position = "Accountant";
		emp.salary = 1233.223f;
		System.out.println(emp);
	}
}
