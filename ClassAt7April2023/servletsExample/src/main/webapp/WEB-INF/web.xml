<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>DeleteProductServlet</servlet-name>
  	<display-name>DeleteProductServlet</display-name>
  	<description></description>
  	<servlet-class>servletsExample.DeleteProductServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SessionHandler</servlet-name>
  	<display-name>SessionHandler</display-name>
  	<description></description>
  	<servlet-class>servletsExample.SessionHandler</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CookieHandler</servlet-name>
  	<display-name>CookieHandler</display-name>
  	<description></description>
  	<servlet-class>servletsExample.CookieHandler</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>DeleteProductServlet</servlet-name>
  	<url-pattern>/DeleteProductServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SessionHandler</servlet-name>
  	<url-pattern>/sessionHandler</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CookieHandler</servlet-name>
  	<url-pattern>/cookieHandler</url-pattern>
  </servlet-mapping>

  
  
  
  
  
</web-app>
